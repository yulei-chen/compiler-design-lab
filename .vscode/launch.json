  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Main",
      "request": "launch",
      "mainClass": "edu.kit.kastel.vads.compiler/edu.kit.kastel.vads.compiler.Main",
      "projectName": "compiler-design-lab"
    },
    {
      "type": "java",
      "name": "Test with Graph Dump",
      "request": "launch",
      "mainClass": "edu.kit.kastel.vads.compiler/edu.kit.kastel.vads.compiler.Main",
      "projectName": "compiler-design-lab",
      "args": ["tests/test.c", "tests/test"],
      "env": {
        "DUMP_GRAPHS": "dot"
      },
      "preLaunchTask": "build"
    }
  ]
}
